                android:layout_alignParentRight="true"
                android:paddingRight="10dp"
                style="@style/gray16Text" />
              <Button
                    android:id="@+id/bt_v3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/more_item_selector" />
            
        </RelativeLayout>


    <RelativeLayout
            android:id="@+id/rl_szcity"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="1dip"
            android:background="@drawable/button_p_kuang4"
            android:padding="10dp"
            android:layout_gravity="center"
 
             >

            <TextView 
                style="@style/black18Text"
                android:textColor="@color/new_black_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/khyh_city"
                 />

            <TextView
                android:id="@+id/tv_szcity"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toLeftOf="@+id/bt_v4"
                android:text="@string/sw_bandding"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:paddingRight="10dp"
                style="@style/gray16Text" />
