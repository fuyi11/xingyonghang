                android:text="@string/sw_bandding"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:paddingRight="10dp"
                style="@style/gray16Text" />
              <Button
                    android:id="@+id/bt_v5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:background="@drawable/more_item_selector" />
            
        </RelativeLayout>
   
    <Button
        android:id="@+id/btn_yhbind"
        style="@style/white18Text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/new_btn_shape"
        android:padding="10dp"
        android:text="@string/wbd" />
    <!--  <Button
        android:id="@+id/btn_updatebank"
        style="@style/white18Text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/button_b_back1"
        android:paddingRight="5dp"
        android:text="@string/update_wbd"
        android:visibility="gone" />-->

</LinearLayout>
